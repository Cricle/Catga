{
  "metadata": [],
  "build": {
    "markdownEngineName": "markdig",
    "markdownEngineProperties": {
      "noYamlHeader": true
    },
    "content": [
      {
        "files": [
          "docs/**.md",
          "docs/toc.yml"
        ],
        "exclude": [
          "AI-VIEW.md"
        ]
      },
      {
        "files": [
          "examples/**.md"
        ]
      },
      {
        "files": [
          "src/Catga.Testing/README.md",
          "scripts/README.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "docs/images/**",
          "docs/web/**",
          "examples/**",
          "scripts/**"
        ],
        "exclude": [
          "docs/web/**.md",
          "examples/**.md",
          "scripts/**.md"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Catga Documentation",
      "_appName": "Catga",
      "_appFooter": "Â© 2025 Catga Framework",
      "_enableSearch": true
    },
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}

