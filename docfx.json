{
  "metadata": [
    {
      "src": [
        {
          "src": "src",
          "files": [
            "Catga/Catga.csproj",
            "Catga.AspNetCore/Catga.AspNetCore.csproj",
            "Catga.Transport.InMemory/Catga.Transport.InMemory.csproj",
            "Catga.Transport.Nats/Catga.Transport.Nats.csproj",
            "Catga.Transport.Redis/Catga.Transport.Redis.csproj",
            "Catga.Persistence.InMemory/Catga.Persistence.InMemory.csproj",
            "Catga.Persistence.Nats/Catga.Persistence.Nats.csproj",
            "Catga.Persistence.Redis/Catga.Persistence.Redis.csproj",
            "Catga.Serialization.MemoryPack/Catga.Serialization.MemoryPack.csproj"
          ]
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net9.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "docs/**.md",
          "docs/toc.yml"
        ],
        "exclude": [
          "AI-VIEW.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "docs/images/**",
          "docs/web/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadata": {
      "_appTitle": "Catga Documentation",
      "_appName": "Catga",
      "_appFooter": "Â© 2025 Catga Framework",
      "_enableSearch": true
    },
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}

