{
  "dashboard": {
    "title": "Catga CQRS Framework - Production Monitoring",
    "description": "È´òÊÄßËÉΩ CQRS Ê°ÜÊû∂ÁõëÊéß‰ª™Ë°®Êùø - Á∫≥ÁßíÁ∫ßÂª∂Ëøü, 2M+ QPS",
    "tags": ["catga", "cqrs", "performance", "aot"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 2,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "üìä Command Execution Rate (QPS)",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(catga_commands_executed_total[1m])",
            "legendFormat": "{{request_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 1000000, "color": "yellow" },
                { "value": 2000000, "color": "red" }
              ]
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["lastNotNull", "max"]
          }
        }
      },
      {
        "id": 2,
        "title": "‚úÖ Command Success Rate",
        "type": "stat",
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(catga_commands_executed_total{success=\"true\"}[5m])) / sum(rate(catga_commands_executed_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 95, "color": "yellow" },
                { "value": 99, "color": "green" },
                { "value": 99.9, "color": "blue" }
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 3,
        "title": "üì® Event Publishing Rate",
        "type": "stat",
        "gridPos": { "x": 18, "y": 0, "w": 6, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(catga_events_published_total[1m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 0,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 2000000, "color": "yellow" }
              ]
            }
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 4,
        "title": "‚ö° Command Duration (p50, p95, p99) - ÁõÆÊ†á: < 1Œºs",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(catga_command_duration_milliseconds_bucket[5m])) * 1000",
            "legendFormat": "p50 - {{request_type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(catga_command_duration_milliseconds_bucket[5m])) * 1000",
            "legendFormat": "p95 - {{request_type}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(catga_command_duration_milliseconds_bucket[5m])) * 1000",
            "legendFormat": "p99 - {{request_type}}",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "¬µs",
            "decimals": 2,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 1, "color": "yellow" },
                { "value": 10, "color": "red" }
              ]
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "lastNotNull", "max"]
          }
        }
      },
      {
        "id": 5,
        "title": "‚ùå Error Rate by Type",
        "type": "timeseries",
        "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(catga_commands_executed_total{success=\"false\"}[5m])",
            "legendFormat": "{{request_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "custom": {
              "drawStyle": "bars",
              "fillOpacity": 70
            },
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 6,
        "title": "üîÑ Concurrency Limiter Usage",
        "type": "gauge",
        "gridPos": { "x": 0, "y": 16, "w": 6, "h": 6 },
        "targets": [
          {
            "expr": "catga_concurrency_current / catga_concurrency_limit * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 70, "color": "yellow" },
                { "value": 90, "color": "red" }
              ]
            },
            "max": 100,
            "min": 0
          }
        },
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 7,
        "title": "üõ°Ô∏è Circuit Breaker Status",
        "type": "stat",
        "gridPos": { "x": 6, "y": 16, "w": 6, "h": 6 },
        "targets": [
          {
            "expr": "catga_circuit_breaker_state",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "mappings": [
              { "type": "value", "value": 0, "text": "‚úÖ Closed", "color": "green" },
              { "type": "value", "value": 1, "text": "üî¥ Open", "color": "red" },
              { "type": "value", "value": 2, "text": "‚ö†Ô∏è Half-Open", "color": "yellow" }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 1, "color": "red" },
                { "value": 2, "color": "yellow" }
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "textMode": "value"
        }
      },
      {
        "id": 8,
        "title": "üìà Top 10 Commands by Execution Count",
        "type": "table",
        "gridPos": { "x": 12, "y": 16, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "topk(10, sum by (request_type) (increase(catga_commands_executed_total[5m])))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {
                "request_type": 0,
                "Value": 1
              },
              "renameByName": {
                "request_type": "Command Type",
                "Value": "Executions (5m)"
              }
            }
          }
        ]
      },
      {
        "id": 9,
        "title": "üíæ Memory Allocation (GC Pressure)",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 22, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "rate(dotnet_gc_collection_seconds_total{generation=\"0\"}[1m])",
            "legendFormat": "Gen0 GC",
            "refId": "A"
          },
          {
            "expr": "rate(dotnet_gc_collection_seconds_total{generation=\"1\"}[1m])",
            "legendFormat": "Gen1 GC",
            "refId": "B"
          },
          {
            "expr": "rate(dotnet_gc_collection_seconds_total{generation=\"2\"}[1m])",
            "legendFormat": "Gen2 GC",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 10,
        "title": "üöÄ Throughput Summary (Last 5m)",
        "type": "stat",
        "gridPos": { "x": 12, "y": 22, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "sum(rate(catga_commands_executed_total[5m]))",
            "legendFormat": "Commands/s",
            "refId": "A"
          },
          {
            "expr": "sum(rate(catga_events_published_total[5m]))",
            "legendFormat": "Events/s",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "text" },
                { "value": 1000000, "color": "green" },
                { "value": 2000000, "color": "blue" }
              ]
            }
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value_and_name",
          "orientation": "horizontal"
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus",
          "label": "Data Source"
        },
        {
          "name": "namespace",
          "type": "query",
          "query": "label_values(catga_commands_executed_total, namespace)",
          "datasource": "${datasource}",
          "label": "Namespace",
          "multi": false,
          "includeAll": true
        },
        {
          "name": "interval",
          "type": "interval",
          "query": "1m,5m,10m,30m,1h",
          "label": "Interval",
          "auto": true,
          "auto_count": 30,
          "auto_min": "10s"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "${datasource}",
          "enable": true,
          "iconColor": "blue",
          "tags": ["deployment"]
        }
      ]
    }
  }
}
