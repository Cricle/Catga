<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <LangVersion>preview</LangVersion>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <IsAotCompatible>false</IsAotCompatible>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>Catga.Debugger</PackageId>
    <Version>0.1.0-preview</Version>
    <Authors>Catga Contributors</Authors>
    <Description>Time-travel debugging and replay system for Catga - production-ready with zero-overhead design</Description>
    <PackageTags>catga;cqrs;debugging;time-travel;replay;diagnostics</PackageTags>
    <PackageProjectUrl>https://github.com/catga/catga</PackageProjectUrl>
    <RepositoryUrl>https://github.com/catga/catga</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Catga\Catga.csproj" />
    <ProjectReference Include="..\Catga.InMemory\Catga.InMemory.csproj" />
  </ItemGroup>

</Project>

