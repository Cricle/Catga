<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net9.0;net8.0</TargetFrameworks>
    <Description>MemoryPack serialization for Catga - AOT compatible binary serialization</Description>
    <PackageTags>$(PackageTags);memorypack;serialization;binary</PackageTags>
    <PublishAot>true</PublishAot>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Catga\Catga.csproj" />
    <PackageReference Include="MemoryPack" />
  </ItemGroup>

</Project>
