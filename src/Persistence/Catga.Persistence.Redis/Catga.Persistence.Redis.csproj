<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <IsAotCompatible>true</IsAotCompatible>

    <!-- Package Info -->
    <PackageId>Catga.Persistence.Redis</PackageId>
    <Title>Catga Redis Persistence</Title>
    <Description>Redis-based persistence implementations for Catga CQRS framework - distributed lock, cache, event store, outbox/inbox, and message transport with Redis Streams</Description>
    <PackageTags>catga;cqrs;redis;persistence;distributed-lock;cache;event-sourcing;outbox;inbox;streams</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Catga\Catga.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="StackExchange.Redis" />
  </ItemGroup>

</Project>
