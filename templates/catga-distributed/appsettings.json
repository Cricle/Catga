{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Catga": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Database": "Server=localhost;Database=catga;User=sa;Password=YourPassword123;",
    "Redis": "localhost:6379"
  },
  "DistributedId": {
    "WorkerId": 1,
    "DataCenterId": 1
  },
  "Nats": {
    "Url": "nats://localhost:4222",
    "EnableJetStream": true
  },
  "Catga": {
    "MaxConcurrency": 1000,
    "DefaultTimeout": "00:00:30",
    "CircuitBreaker": {
      "FailureThreshold": 5,
      "SuccessThreshold": 2,
      "Timeout": "00:00:30"
    }
  }
}

